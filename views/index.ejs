<div class="maincontent-area">
<div class="zigzag-bottom"></div>
<div class="container">
    <div class="row">
        <div class="col-md-12">
            <div class="latest-product">
                <h2 class="section-title">Latest Products</h2>
                <div class="product-carousel">
                    <% for(var i=0; i <15; i++) { %>
                     
                    <div class="single-product">
                        <div class="product-f-image">
                            <img  src="<%= products[i].thumbnail %>" alt="">
                            <div class="product-hover">
                                <a href="#" class="add-to-cart-link"><i class="fa fa-shopping-cart"></i> Add to cart</a>
                                <a href="\details\<%=products[i].id %>" class="view-details-link"><i class="fa fa-link"></i> See details</a>
                            </div>
                        </div>
                        
                        <h2><a href="\details\<%=products[i].id %>"><%= products[i].title  %></a></h2>
                        
                        <div class="product-carousel-price">
                            <% var discount=(products[i].price *products[i].discountPercentage)/100  %>
                             <ins><%= discount   %></ins> <del><%=   products[i].price %></del>
                        </div> 
                    </div>
                    <% } %>
                   
                </div> 
            </div>
        </div>
    </div>
</div>
</div> <!-- End main content area -->
